        <label>CertName</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>EinsteinUsername__c</fullName>
        <description>username (probably an email address) that you used to sign up for the Eintstein predictive vision service</description>
        <externalId>false</externalId>
        <inlineHelpText>username (probably an email address) that you used to sign up for the Eintstein predictive vision service</inlineHelpText>
        <label>EinsteinUsername</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>LocalEinsteinUser__c</fullName>
        <description>the salesforce userid (not username) of the user that you want to respond as Einstein</description>
        <externalId>false</externalId>
        <inlineHelpText>the salesforce userid (not username) of the user that you want to respond as Einstein</inlineHelpText>
        <label>LocalEinsteinUser</label>
        <length>18</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
